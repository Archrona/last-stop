{
    "group": "idioms_jest",
    
    "commands": [
        {
            "spoken": ["jest test case", "just test case"],
            "insert": "test($_, () => {$u$_$d});$n$_"
        },
        {
            "spoken": ["expect to be"],
            "insert": "expect($_)$_.toBe($_)"
        },
        {
            "spoken": ["expect to equal", "expect to equals"],
            "insert": "expect($_)$_.toEqual($_)"
        },
        {
            "spoken": ["expect to throw"],
            "insert": "expect(() => {$u$_$d}).toThrow($_)"
        }
    ]
}
