{
    "name": "single_quoted_string",
    "extensions": [],
    "tokens": [
        {
            "type": "white",
            "pattern": "[ \\t]+"
        },
        {
            "type": "newline",
            "pattern": "\\r|\\n|\\r\\n" 
        },
        {
            "type": "string_escape",
            "pattern": "\\\\."
        },
        {
            "type": "string_text",
            "pattern": "[^ '\\t\\\\]+" 
        }
    ],
    "commands": [
        "universal",
        "string_escapes",
        "eol_semi"
    ],
    "contextChanges": [
        {
            "token": "'",
            "action": "pop",
            "style": "string_delimiter"
        }
    ],
    "rawInput": true,
    "defaultCasing": "raw",
    "spacing": [
        {
            "what": "never automatically insert space",
            "rule": false
        }
    ]
}